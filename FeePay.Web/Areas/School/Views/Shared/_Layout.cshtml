
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="@(ViewBag.MetaDescription != null ? ViewBag.MetaDescription : "Feepay")">
    <meta name="keywords" content="@(ViewBag.MetaKeyWords != null ? ViewBag.MetaKeyWords : "Feepay")">
    <meta name="author" content="@(ViewBag.Author != null ? ViewBag.Author : "travel tech software")">
    <link rel="icon" href="@Url.Content("~/Cuba/images/favicon.png")" type="image/x-icon">
    <link rel="shortcut icon" href="@Url.Content("~/Cuba/images/favicon.png")" type="image/x-icon">
    <title>@(ViewBag.Title != null ? ViewBag.Title + " : ":"") School Admin FeePay</title>
    <link href="https://fonts.googleapis.com/css?family=Rubik:400,400i,500,500i,700,700i&amp;display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,700,700i,900&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/fontawesome.css")">
    <!-- ico-font-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/icofont.css")">
    <!-- Themify icon-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/themify.css")">
    <!-- Flag icon-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/flag-icon.css")">
    <!-- Feather icon-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/feather-icon.css")">
    <!-- Plugins css start-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/animate.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/chartist.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/datatables.css")">
    <!-- Plugins css Ends-->
    <!-- Bootstrap css-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/vendors/bootstrap.css")">
    <!-- App css-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/style.css")">
    <link id="color" rel="stylesheet" href="@Url.Content("~/Cuba/css/color-1.css")" media="screen">
    <!-- Responsive css-->
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Cuba/css/responsive.css")">
    <style>
        #ajaxBusySpin {
            display: none;
            right: 15px;
            position: fixed;
            top: 70px;
            z-index: 100000;
        }

        .input-right-icon {
            position: absolute;
            top: 52px;
            right: 30px;
            -webkit-transform: translateY(-50%);
            transform: translateY(-50%);
            cursor: pointer;
        }

            .input-right-icon.top-20px {
                top: 20px;
                right: 45px;
            }
    </style>
    @RenderSection("Style", false)
</head>
<body>
    <div id="ajaxBusySpin" class="fa-3x" style="display:none"><i class="fa fa-spinner fa-pulse"></i></div>
    <!-- tap on top starts-->
    <div class="tap-top"><i data-feather="chevrons-up"></i></div>
    <!-- tap on tap ends-->
    <!-- page-wrapper Start-->
    <div class="page-wrapper compact-wrapper" id="pageWrapper">
        <!-- Page Header Start-->
        <partial name="_Partial_LayoutHeader" />
        <!-- Page Header Ends-->
        <!-- Page Body Start-->
        <div class="page-body-wrapper sidebar-icon">
            <!-- Page Sidebar Start-->
            <partial name="_Partial_LayoutLeftMenu" />
            <!-- Page Sidebar Ends-->
            <div class="page-body">
                @RenderBody()
            </div>
            <!-- footer start-->
            <partial name="_Partial_LayoutFooter" />
            <!-- footer ends-->
        </div>
        <!-- Page Body Ends-->
    </div>
    <!-- page-wrapper ends-->
    <!-- bootstrap modal-->
    <partial name="_Partial_ShowImageModal" />
    @RenderSection("Models", false)
    <partial name="_Partial_ChangeSessionModal.cshtml" />
    <!-- bootstrap modal ends-->
    <!-- latest jquery-->
    <script src="@Url.Content("~/Cuba/js/jquery-3.5.1.min.js")"></script>
    <!-- Bootstrap js-->
    <script src="@Url.Content("~/Cuba/js/bootstrap/popper.min.js")"></script>
    <script src="@Url.Content("~/Cuba/js/bootstrap/bootstrap.js")"></script>
    <!-- feather icon js-->
    <script src="@Url.Content("~/Cuba/js/icons/feather-icon/feather.min.js")"></script>
    <script src="@Url.Content("~/Cuba/js/icons/feather-icon/feather-icon.js")"></script>
    <!-- Sidebar jquery-->
    <script src="@Url.Content("~/Cuba/js/config.js")"></script>
    <!-- Plugins JS start-->
    <script src="@Url.Content("~/Cuba/js/sidebar-menu.js")"></script>
    <script src="@Url.Content("~/Cuba/js/tooltip-init.js")"></script>
    <script src="@Url.Content("~/Cuba/js/clipboard/clipboard.min.js")"></script>
    <script src="@Url.Content("~/Cuba/js/custom-card/custom-card.js")"></script>
    <script src="@Url.Content("~/Cuba/js/notify/bootstrap-notify.min.js")"></script>
    <script src="@Url.Content("~/Cuba/js/datatable/datatables/jquery.dataTables.min.js")"></script>
    <!-- Plugins JS Ends-->
    <partial name="_Partial_AlertMessage" />
    <!-- Theme js-->
    <script src="@Url.Content("~/Cuba/js/script.js")"></script>
    <script src="@Url.Content("~/Page_js/index_setting.js")"></script>
    <script src="@Url.Content("~/Page_js/index_global.js")"></script>
    @*<script src="@Url.Content("~/Cuba/js/theme-customizer/customizer.js")"></script>*@
    <!-- login js-->
    <!-- Plugin used-->
    @RenderSection("Scripts", false)
</body>
</html>
